<style>
    /*
   页脚样式
   */
    .g-footer{
        z-index: 2;
        position: fixed;
        bottom: 0;
        width: 100%;
        max-width: 640px;
        padding: 1.5% 0;
        background-color: #fff;
        border-top: 1px solid #ddd;
    }
    .g-footer .item{
        float: left;
        width: 33%;
        text-align: center;
    }
    .g-footer .item a {
        display: block;
        padding: 3% 0;
        border-left: 1px solid #fff;
        border-right: 1px solid #cecece;
        color: #7b7b7b;
        font-size: 12px;
        line-height: 1.8em;
    }
    .g-footer .item:nth-child(1) a {
        border-left: none;
    }
    .g-footer .item:nth-child(3) a {
        border-right: none;
    }
    .g-footer .item a.nav-active{
        color: #ff6600;
    }

    .g-view {
        -webkit-transition: all .3s ease;
        transition: all .3s ease;
        position: absolute;
        display: block;
        width: 100%;
        height: 100%;
    }
    .fade-enter{
        opacity: 1;
        transform: translate3d(100%, 0, 0);
    }

    .fade-leave {
        opacity: 0;
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);

    }

    .back-enter {
        opacity: 1;
        -webkit-transform: translate3d(-110%, 0, 0);
        transform: translate3d(-110%, 0, 0);

    }
    .back-leave{
        opacity: 0;
        /*transform: translate3d(100%, 0, 0);*/
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);
    }

    .bounce-enter {
        animation: bounce-in .5s;
    }
    .bounce-leave {
        animation: bounce-out .5s;
    }
    @keyframes bounce-in {
        0% {
            transform: scale(0);
        }
        50% {
            transform: scale(1.5);
        }
        100% {
            transform: scale(1);
        }
    }
    @keyframes bounce-out {
        0% {
            transform: scale(1);
        }
        50% {
            transform: scale(1.5);
        }
        100% {
            transform: scale(0);
        }
    }

    .card-scroll {
        height: 100%;
    }
    .card-active {
    }
    .card-page {
        width: 100%;
        position: absolute;
    }
</style>
